{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "checkJs": true,
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*":["./src/assets/*"],
      "@components/*":["./src/components/*"],
      "@constants/*":["./src/constants/*"],
      "@routes/*":["./src/routes/*"],
      "@screens/*":["./src/screens/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
